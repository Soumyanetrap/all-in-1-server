{
    "version": 2,
    "builds": [
        {
            "src": "./home.js",
            "use": "@vercel/node",
            "config": {
                "maxDuration": 60 
            }
        }
    ],
    "routes": [
    {
        "src": "/(.*)",
        "dest": "/home.js",
        "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
        "headers": {
            "Access-Control-Allow-Origin": "*"
        }
    }
    ]
}